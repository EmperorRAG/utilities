{
	"name": "@emperorrag/utilities",
	"description": "A collection of utilities for testing and development in TypeScript projects.",
	"private": false,
	"version": "0.0.2",
	"author": "",
	"license": "MIT",
	"type": "module",
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/EmperorRAG/utilities.git"
	},
	"homepage": "https://github.com/EmperorRAG/utilities#readme",
	"bugs": {
		"url": "https://github.com/EmperorRAG/utilities/issues"
	},
	"keywords": [
		"typescript",
		"utilities",
		"testing"
	],
	"main": "dist/utilities.umd.cjs",
	"module": "dist/utilities.js",
	"types": "dist/utilities.d.ts",
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/utilities.js",
			"require": "./dist/utilities.umd.cjs"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build && npm run build:types",
		"build:types": "tsc --project tsconfig.types.json",
		"lint": "eslint .",
		"preview": "vite preview",
		"test": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@vitest/coverage-v8": "^3.2.4",
		"dts-bundle-generator": "^9.5.1",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"prettier": "3.6.2",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.39.1",
		"vite": "^7.1.2",
		"vitest": "^3.2.4"
	},
	"peerDependencies": {
		"vite": "^7.1.2",
		"vitest": "^3.2.4"
	}
}
